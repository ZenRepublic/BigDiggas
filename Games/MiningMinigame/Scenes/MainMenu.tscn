script/source = "extends PanelContainer

@onready var back_button:BaseButton = $HBoxContainer/MarginContainer/BackButton
# Called when the node enters the scene tree for the first time.

func _on_rubian_mint_screen_on_nft_mint_started() -> void:
	back_button.disabled=true

func _on_rubian_mint_screen_on_nft_minted(_nft_account: Pubkey) -> void:
	back_button.disabled=false
	
func _on_rubian_mint_screen_on_nft_mint_failed() -> void:
	back_button.disabled=false
"

[sub_resource type="GDScript" id="GDScript_28bmg"]
script/source = "extends PanelContainer

@onready var rubian:TextureRect = $VBoxContainer/Visual/RubianMeditation
@onready var animator:AnimationPlayer = $AnimationPlayer

var rubian_position_values:Array[Vector2] = [Vector2(0,35),Vector2(0,-35)]
# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	animator.play(\"MeditationText\")
	await get_tree().process_frame
	meditate_tween()

func meditate_tween() -> void:
	var tween:Tween = get_tree().create_tween()
	tween.tween_property(rubian,\"position\",rubian_position_values[0],1.0).set_trans(Tween.TRANS_SINE)
	await tween.finished
	
	rubian_position_values.reverse()
	meditate_tween()
"

